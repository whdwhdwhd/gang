<!--pages/aboutUs/aboutUs.wxml-->
<view class="aboutUs">
  <view class="aboutUs-usImagtPath">
    <image src="{{aboutUs.usImagtPath}}"></image>
  </view>
  <view class="aboutUs-usName">{{aboutUs.usName}}</view>
  <view class="aboutUs-usLxr"><text>联系人：</text>{{aboutUs.usLxr}}</view>
  <view class="aboutUs-usPhone"><text>手机号：</text>{{aboutUs.usPhone}}</view>
  <view class="aboutUs-usWx"><text>微信号：</text>{{aboutUs.usWx}}</view>
  <view class="aboutUs-usMail"><text>邮箱：</text>{{aboutUs.usMail}}</view>
  <view class="aboutUs-usAddress"><text>地址：</text>{{aboutUs.usAddress}}</view>
  <view class="aboutUs-usDesc"><text>简介：</text>{{aboutUs.usDesc}}</view>
</view>
